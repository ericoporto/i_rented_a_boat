// room script file

function room_AfterFadeIn()
{
  CustomTransition_FadeIn(0.2, eBlockTween, 0.0, eAgsColor_White);
  dBadEnd1.Start();
}

function room_Load()
{
  bool_in_end = true;
  gWhite.Visible = true;
}

function room_RepExec()
{
  if(bool_to_intro_d2) {
    IntroStack.Reset();
    Wait(SecondsToLoops(0.5));
    Wait(SecondsToLoops(1.0));
    dBadEnd2.Start();
  }
  if(!bool_in_end) {
    IntroStack.Reset();
    Wait(SecondsToLoops(1.0));
    player.ChangeRoom(36);
  }
}
